{
    "name": "onefasteuro/shopifyapps",
    "description": "Authentication & Billing with the Shopify API",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Haroun",
            "email": "onefasteuro@gmail.com",
            "homepage": "https://driftfreediving.com"
        }
    ],
    "homepage": "https://github.com/onefasteuro/shopifyapps",
    "keywords": ["Laravel", "Shopify", "OAuth", "Billing"],
    "require": {
        "ext-json": "*",
        "ext-curl": "*",
        "illuminate/support": "^6.0",
        "illuminate/config": "^6.0",
        "illuminate/http": "^6.0",
        "illuminate/events": "^6.0",
        "onefasteuro/shopifyclient": "dev-master",
        "onefasteuro/shopifyutils": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^4.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "onefasteuro\\ShopifyApps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "onefasteuro\\ShopifyApps\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "onefasteuro\\ShopifyApps\\ShopifyAppsServiceProvider"
            ]
        }
    }
}
